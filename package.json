{
  "name": "gramquiz",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "version": "1.0.0",
  "description": "This quiz evaluates personality type by letting user choose words and based on words chosen and attributes attached to each word selected evaluate to a personality typology grouped into 9 types and subtypes based on the Enneagram Typology.",
  "main": "./js/app.js",
  "scripts": {
    "prod": "node ./start.js",
    "watch": "nodemon server --ignore public/",
    "start": "concurrently 'npm run watch' 'npm run assets' --names  --prefix name/",
    "assets": "webpack -w --display-max-modules 0",
    "build": "webpack",
    "test": "mocha --timeout 10000",
    "now": "now -e DB_USER=@db_user -e DB_PASS=@db_pass -e NODE_ENV=\"production\" -e PORT=80"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yodacom/gramQuiz.git"
  },
  "author": "Jeremy Black",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yodacom/gramQuiz/issues"
  },
  "homepage": "https://github.com/yodacom/gramQuiz#readme",
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "axios": "^0.16.2",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "concurrently": "^3.4.0",
    "connect-flash": "^0.1.1",
    "crypto": "^0.0.3",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "mocha": "^3.4.2",
    "mongoose": "^4.10.5",
    "mongoose-promises": "^0.0.6",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "sinon": "^2.3.2",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "chai": "^4.0.1",
    "chai-http": "^3.0.0",
    "should": "^11.2.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
